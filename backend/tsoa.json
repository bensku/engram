{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controller/*"],
  "spec": {
    "outputDirectory": "generated",
    "specVersion": 3,
    "securityDefinitions": {
      "auth": {
        "type": "oauth2",
        "authorizationUrl": "",
        "flow": "implicit",
        "scopes": {}
      }
    }
  },
  "routes": {
    "routesDir": "generated",
    "middleware": "koa",
    "authenticationModule": "./src/auth.ts"
  }
}