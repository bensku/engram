{
  "name": "@bensku/engram-backend",
  "description": "",
  "scripts": {
    "openapi-clients": "concurrently 'openapi-typescript vendor/schema/openai.yaml --output generated/openai.d.ts' 'openapi-typescript vendor/schema/qdrant.json --output generated/qdrant.d.ts' 'openapi-typescript vendor/schema/together.yaml --output generated/together.d.ts'",
    "routes": "tsoa spec-and-routes",
    "postinstall": "npm run openapi-clients && npm run routes",
    "dev": "concurrently 'nodemon --config nodemon.app.json' 'nodemon --config nodemon.tsoa.json'",
    "migrations": "drizzle-kit generate:pg",
    "build": "tsc"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@aws-sdk/client-bedrock-runtime": "^3.529.0",
    "@koa/router": "^12.0.1",
    "@qdrant/js-client-rest": "^1.8.0",
    "ajv": "^8.12.0",
    "cohere-ai": "^7.7.7",
    "drizzle-orm": "^0.30.1",
    "dumpster-dive": "^5.6.3",
    "fast-xml-parser": "^4.3.5",
    "koa": "^2.15.0",
    "koa-body": "^6.0.1",
    "koa-passport": "^6.0.0",
    "koa-session": "^6.4.0",
    "mongodb": "^6.4.0",
    "nodemw": "^0.21.0",
    "openid-client": "^5.6.1",
    "postgres": "^3.3.4",
    "tsoa": "^6.0.1"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.1",
    "@types/koa-passport": "^6.0.3",
    "@types/koa-session": "^6.4.5",
    "@types/node": "^20.11.25",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}
